{
  "name": "@transformgovsg/pdf2md",
  "version": "0.1.1",
  "description": "A CLI tool that converts PDF files into Markdown using Azure Document Intelligence for text extraction and OpenAI for Markdown formatting.",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/transformgovsg/pdf2md.git"
  },
  "license": "AGPL-3.0-only",
  "files": [
    "dist",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "bin": {
    "pdf2md": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "node --import=tsx src/index.ts",
    "format": "prettier --write \"**/*.{js,mjs,ts,md,json}\"",
    "lint": "eslint \"**/*.{js,mjs,ts}\"",
    "typecheck": "tsc",
    "prepare": "husky"
  },
  "dependencies": {
    "@azure-rest/ai-document-intelligence": "^1.0.0",
    "arg": "^5.0.2",
    "chalk": "^5.4.1",
    "dedent": "^1.5.3",
    "dotenv": "^16.4.7",
    "openai": "^4.86.2",
    "wrap-ansi": "^9.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/node": "^22.13.10",
    "eslint": "^9.22.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0"
  },
  "packageManager": "pnpm@10.6.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "overrides": {
      "node-fetch@2.x>whatwg-url": "^14.0.0"
    }
  }
}
