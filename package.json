{
  "name": "@transformgovsg/pdf2md",
  "version": "0.0.0",
  "description": "Convert PDF to Markdown with Azure Document Intelligence and OpenAI LLM",
  "type": "module",
  "license": "AGPL-3.0-only",
  "files": [
    "dist",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "node --import=tsx src/index.ts",
    "format": "prettier --write \"**/*.{js,mjs,ts,md,json}\""
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "packageManager": "pnpm@10.6.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
